{
  "name": "my-micro-front-end-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall:bak": "ngcc",
    "postinstall": "ngcc",
    "build:headerApp:externals": "ng build --extra-webpack-config projects/header-app//webpack.externals.js --prod --project headerApp --single-bundle",
    "build:footerApp:externals": "ng build --extra-webpack-config projects/footer-app//webpack.externals.js --prod --project footerApp --single-bundle",
    "build:subApp:externals": "ng build --extra-webpack-config projects/sub-app//webpack.externals.js --prod --project subApp --single-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "^13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "^13.1.0",
    "@angular/elements": "^13.1.3",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "ngx-build-plus": "^14.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "~13.1.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
